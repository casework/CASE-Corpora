@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix dctype: <http://purl.org/dc/dcmitype/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix sh-dcterms: <http://example.org/shapes/dcterms/> .

<http://example.org/shapes/dcterms>
	a owl:Ontology ;
	.

sh-dcterms:accrualPeriodicity-domain-shape
	a sh:NodeShape ;
	rdfs:seeAlso <https://www.dublincore.org/specifications/dublin-core/dcmi-terms/#http://purl.org/dc/terms/accrualPeriodicity> ;
	sh:property [
		a sh:PropertyShape ;
		sh:hasValue dctype:Collection ;
		sh:path rdf:type ;
	] ;
	sh:targetSubjectsOf dcterms:accrualPeriodicity ;
	.

sh-dcterms:format-shape
	a sh:NodeShape ;
	rdfs:seeAlso <https://dublincore.org/specifications/dublin-core/dcmi-terms/#format> ;
	sh:property [
		a sh:PropertyShape ;
		sh:nodeKind sh:BlankNodeOrIRI ;
		sh:or (
			[
				sh:class dcterms:Extent ;
			]
			[
				sh:class dcterms:MediaType ;
			]
		) ;
		sh:path dcterms:format ;
	] ;
	sh:targetSubjectsOf dcterms:format ;
	.

sh-dcterms:language-shape
	a sh:NodeShape ;
	rdfs:seeAlso <https://www.dublincore.org/specifications/dublin-core/dcmi-terms/#http://purl.org/dc/terms/language> ;
	sh:property [
		a sh:PropertyShape ;
		sh:nodeKind sh:IRIOrLiteral ;
		sh:path dcterms:language ;
	] ;
	sh:targetSubjectsOf dcterms:language ;
	.

sh-dcterms:modified-shape
	a sh:NodeShape ;
	rdfs:seeAlso <https://www.dublincore.org/specifications/dublin-core/dcmi-terms/#http://purl.org/dc/terms/modified> ;
	sh:property [
		a sh:PropertyShape ;
		sh:nodeKind sh:Literal ;
		sh:path dcterms:modified ;
	] ;
	sh:targetSubjectsOf dcterms:modified ;
	.

